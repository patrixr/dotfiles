{
  "outline_panel": {
    "dock": "right"
  },
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4o"
    },
    "version": "2"
  },
  "cursor_shape": "block",
  "ui_font_size": 15,
  "buffer_font_size": 13,
  "current_line_highlight": "all",
  "edit_predictions": {
    "mode": "subtle"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "tab_size": 2,
  "tab_bar": {
    "show": false,
    "show_nav_history_buttons": false
  },
  "terminal": {
    "toolbar": {
      "breadcrumbs": true
    },
    "shell": {
      "program": "nu"
    },
    "font_size": 13,
    "dock": "right",
    "scrollbar": {
      "show": "system"
    }
  },
  "indent_guides": {
    "enabled": true,
    "coloring": "indent_aware",
    "background_coloring": "disabled",
    "active_line_width": 2
  },
  "scrollbar": {
    "show": "auto",
    "cursors": true,
    "git_diff": true,
    "search_results": true,
    "selected_text": true,
    "selected_symbol": true,
    "diagnostics": "all",
    "axes": {
      "horizontal": false,
      "vertical": true
    }
  },
  "format_on_save": "on",
  "code_actions_on_format": {
    "source.fixAll.eslint": true,
    "source.removeUnusedImports": true,
    "source.organizeImports": true
  },
  "languages": {
    "JavaScript": {
      "formatter": {
        "external": {
          "command": "npx",
          "arguments": [
            "--yes",
            "prettier",
            "--stdin-filepath",
            "{buffer_path}"
          ]
        }
      },
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      },
      "format_on_save": "on"
    },
    "TypeScript": {
      "formatter": {
        "external": {
          "command": "npx",
          "arguments": [
            "--yes",
            "prettier",
            "--stdin-filepath",
            "{buffer_path}"
          ]
        }
      },
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      },
      "format_on_save": "on"
    },
    "TSX": {
      "formatter": {
        "external": {
          "command": "npx",
          "arguments": [
            "--yes",
            "prettier",
            "--stdin-filepath",
            "{buffer_path}"
          ]
        }
      },
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      },
      "format_on_save": "on"
    }
  }
}
